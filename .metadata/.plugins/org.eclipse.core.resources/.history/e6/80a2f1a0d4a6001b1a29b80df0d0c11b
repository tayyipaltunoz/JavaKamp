package asalSayi;

public class Main {

	public static void main(String[] args) {
		int sayi = -1;
		boolean asalMi = true;

		if (sayi == 1 ) {
			System.out.println("Sayi deðildir");
			return;
			
		}
		if (sayi < 1 ) {
				System.out.println("Sayi asal deðildir");
				return;
		}

		for (int i = 2; i < sayi; i++) {
			if (sayi % i == 0) {

				asalMi = false;
			}

		}
		if (asalMi) {
			System.out.println("Sayi asaldýr");
		} else {
			System.out.println("Sayi asal deðildir");

		}

	}
}
