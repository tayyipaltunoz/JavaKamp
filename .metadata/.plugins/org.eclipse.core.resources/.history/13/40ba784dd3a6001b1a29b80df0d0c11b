package asalSayi;

public class Main {

	public static void main(String[] args) {
		int sayi = 5;
		boolean asalMi = true;

		for (int i = 2; i <= sayi; i++) {
			if (sayi % i == 0) {

				asalMi = false;
			}
			if (asalMi) {
				System.out.println("Sayi asaldýr");
			} else {
				System.out.println("Sayi asaldýr deðildir");

			}
		}

	}
}
