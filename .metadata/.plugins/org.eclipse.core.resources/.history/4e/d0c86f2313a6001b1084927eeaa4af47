package multiDimensionalArrayDemo;

public class Main {

	public static void main(String[] args) {
		String[][] sehirler = new String[3][3];
		
		sehirler[0][0]="Ýstanbul";
		sehirler[0][1]="Ýstanbul";
		sehirler[0][2]="Ýstanbul";
		sehirler[1][0]="Ankara";
		sehirler[1][1]="Ankara";
		sehirler[1][2]="Ankara";
		sehirler[2][0]="Bursa";
		sehirler[2][1]="Bursa";
		sehirler[3][2]="Bursa";
	
		

	}

}
