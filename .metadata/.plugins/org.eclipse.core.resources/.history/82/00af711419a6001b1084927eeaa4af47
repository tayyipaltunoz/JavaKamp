package stringsDemo;

public class Main {

	public static void main(String[] args) {
		String mesaj = "Merhaba nasýlsýn?";

		System.out.println("Eleman sayýsý : " + mesaj.length());
		System.out.println("Beþinci eleman : " + mesaj.charAt(5));
		System.out.println(mesaj.concat(" Ýyiyim ya sen"));
		System.out.println(mesaj.startsWith("M"));
		System.out.println(mesaj.endsWith("M"));
	}

}
